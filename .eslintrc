{
    "extends": [
        "airbnb",
        "react-app"
    ],
    "rules": {
        "jsx-a11y/href-no-hash": "off",
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": 0,
        "jsx-a11y/anchor-is-valid": [
            "warn",
            {
                "aspects": [
                    "invalidHref"
                ]
            }
        ]
    }
}